{
  "name": "@clasificador-acentos/backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && prisma generate",
    "start": "node dist/index.js",
    "postbuild": "prisma migrate deploy && tsx dist/database/run-seed.js",
    "test": "jest",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx src/database/seed.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "axios": "^1.5.0",
    "@prisma/client": "^5.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.14",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "prisma": "^5.3.1",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2"
  }
}